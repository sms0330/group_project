<%= form_for @course do |course| %>
    <div>
        <%= course.label :title %>
        <%= course.text_field :title %>
    </div>
    <div>
        <%= course.label :category %>
        <%= course.text_field :category %>
    </div>
    <div>
        <%= course.label :description %>
        <%= course.text_field :description %>
    </div>
    <div>
        <%= course.label :seats %>
        <%= course.text_field :seats %>
    </div>
    <div>
        <%= course.label :start_date %>
        <%= course.date_field :start_date %>
    </div>
    <div>
        <%= course.label :end_date %>
        <%= course.date_field :end_date %>
    </div>
    <div>
        <%= course.label :time %>
        <%= course.time_field :time %>
    </div>
    <%= course.submit %>
    <% end %>